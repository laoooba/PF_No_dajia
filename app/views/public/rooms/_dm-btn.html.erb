<% if isRoom == true %>
  <%= link_to "チャットへ", room_path(roomID), class:"btn btn-lg btn-outline-primary" %>
<% else %>
  <%= form_with model:room, url: rooms_path, local:true do |f| %>
    <%= fields_for entry do |e| %>
      <%= e.hidden_field :user_id, value: user.id %>
    <% end %>
    <%= f.submit "チャットを始める", class:"btn btn-lg btn-outline-primary mb-3"%>
  <% end %>
<% end %>